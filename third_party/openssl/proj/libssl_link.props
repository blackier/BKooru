<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <libsslIncludePath>$(SolutionDir)third_party\openssl\include\</libsslIncludePath>
  </PropertyGroup>
  <PropertyGroup />
  <ItemDefinitionGroup>
    <ClCompile>
      <PreprocessorDefinitions>ZLIB;ZLIB_SHARED;OPENSSL_SYS_WIN32;WIN32_LEAN_AND_MEAN;_CRT_SECURE_NO_DEPRECATE;_WINSOCK_DEPRECATED_NO_WARNINGS;OPENSSL_USE_APPLINK;LIBZ="ZLIB1";L_ENDIAN;OPENSSL_PIC;OPENSSLDIR="";ENGINESDIR="";%(PreprocessorDefinitions)</PreprocessorDefinitions>
      <AdditionalIncludeDirectories>$(libsslIncludePath);$(libsslIncludePath)openssl\;%(AdditionalIncludeDirectories)</AdditionalIncludeDirectories>
    </ClCompile>
    <Link>
      <AdditionalDependencies>$(OutDir)libssl.lib;ws2_32.lib;crypt32.lib;%(AdditionalDependencies)</AdditionalDependencies>
    </Link>
  </ItemDefinitionGroup>
  <ItemGroup>
    <BuildMacro Include="libsslIncludePath">
      <Value>$(libsslIncludePath)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>